
      generator client {
        provider = "prisma-client-js"
      }
      
      generator trpc {
        provider          = "node ../../lib/generator.js"
        output            = "./generated/import-extensions"
        contextPath       = "../test-context"
        withZod           = true
        withShield        = false
        importExtension   = "js"
        generatedExtension = "mts"
      }
      
      datasource db {
        provider = "sqlite"
        url      = "file:./dev.db"
      }
      
      model User {
        id    Int     @id @default(autoincrement())
        email String  @unique
        name  String?
      }
    